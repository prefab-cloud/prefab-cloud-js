{
  "name": "@prefab-cloud/prefab-cloud-js",
  "version": "0.2.2",
  "description": "Feature Flags & Dynamic Configuration as a Service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Jeffrey Chupp",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/uuid": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.0",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "build": "rm -rf dist/ && ./build.sh",
    "bundle": "npm run build && esbuild --target=esnext --bundle --outfile=dist/prefab.bundle.js --global-name=window.prefabNamespace dist/index.js",
    "lint": "eslint --ext .ts,.tsx src/",
    "lint:fix": "eslint --fix --ext .ts,.tsx src/",
    "postversion": "echo 'postversion' && git push && git push --tags",
    "prepare": "husky install",
    "prettier": "prettier . -l",
    "prettier:fix": "npm run prettier --write .",
    "release": "npm run prettier:fix; npm run lint:fix; npm run test",
    "test": "npm run build && jest --verbose dist/",
    "version": "echo 'version' && npm run release && npm run prettier:fix; git add src/version.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prefab-cloud/prefab-cloud-js.git"
  },
  "keywords": [
    "feature-flags",
    "config"
  ],
  "bugs": {
    "url": "https://github.com/prefab-cloud/prefab-cloud-js/issues"
  },
  "homepage": "https://github.com/prefab-cloud/prefab-cloud-js#readme",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  }
}
